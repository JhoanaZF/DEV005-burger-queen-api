{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "no-shadow": "off",
        "import/no-extraneous-dependencies": ["error", { "devDependencies": ["e2e/**/*.js", "**/*.spec.js"] }],
        "no-console": [
            "error",
            {
                "allow": ["info", "warn", "error"]
            }
        ]
    }
}
